<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="local.test.mybatis.CustomerRepository">
    <select id="findById" parameterType="long" resultType="local.test.mybatis.Customer">
        select * from t_customer where id = #{id}
    </select>
</mapper>
